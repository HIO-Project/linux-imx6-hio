	ipu_id = <0>;
	sec_ipu_id = <0>;

	status = "okay";
};

&mxcfb1 {
        compatible = "fsl,mxc_sdc_fb";
        disp_dev = "lcd";
        interface_pix_fmt = "RGB24";
        mode_str ="480x800";
        default_bpp = <32>;
        int_clk = <0>;
        late_init = <0>;
        status = "okay";
};


