&i2c3 {
	clock-frequency = <400000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3_2>;
	status = "okay";	

        ft6x06_ts@0x38 {
                compatible = "ft,ft6x06_ts";
                reg = <0x38>;
                interrupt-parent = <&gpio1>;
                interrupts = <1 3>;//falling edge
                gpio_int = <&gpio1 1 3>;
                gpio_rst = <&gpio1 4 0>;
        };

