        ist3020-nRst = <&gpio1 8 0>;
        ist3020-A0 = <&gpio1 9 0>;
        ist3020-backlight = <&gpio7 11 0>;
        sata-power = <&gpio4 15 0>;
        usb-hub = <&gpio4 14 0>;
